<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<%= Date.today.year %><%= Date.today.month %>
<%= @month %>
<table>
  <% @posts.each do |post| %>
      <tr>
        <th><%= post.spend_date %></th>
        <th><%= post.title %></th>
        <th><%= post.money %></th>
        <th><%= link_to "edit", edit_post_path(post.id) %></th>
      </tr>
  <% end %>
</table>
<%= link_to "new", new_post_path %>